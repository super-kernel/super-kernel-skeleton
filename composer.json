{
    "name": "super-kernel/super-kernel-skeleton",
    "description": "Project template for the SuperKernel framework.",
    "type": "project",
    "license": "MIT",
    "scripts": {
        "dev": [
            "@composer skernel serve",
            "@php target/runtime/bin.php start"
        ]
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "ext-swoole": ">=6.0",
        "super-kernel/attribute": "v1.0.2",
        "super-kernel/framework": "^1.0",
        "super-kernel/command": "^1.0",
        "super-kernel/http-server": "^1.0",
        "super-kernel/di": "^1.0",
        "super-kernel/event-dispatcher": "^1.0",
        "super-kernel/logger": "^1.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0",
        "super-kernel/composer-plugin": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "super-kernel/composer-plugin": true
        }
    }
}